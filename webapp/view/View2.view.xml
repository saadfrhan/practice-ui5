<mvc:View
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="myapp.controller.View2"
>
    <Page title="View 2" showNavButton="true" navButtonPress="onBack">
        <content>
            <ObjectHeader title="Litchi" icon="https://cdn3d.iconscout.com/3d/premium/thumb/lychee-3d-icon-download-in-png-blend-fbx-gltf-file-formats--delicious-logo-litchi-organic-fruits-pack-food-drink-icons-4873932.png" intro="Peel off to enjoy rich vitamins" number="100 Rs." numberUnit="Kg." ></ObjectHeader>

            <IconTabBar>
                <items>
                    <IconTabFilter text="More Info" icon="sap-icon://warning"></IconTabFilter>
                    <IconTabFilter text="Suppliers" icon="sap-icon://supplier"></IconTabFilter>
                    <IconTabFilter text="Cities" icon="sap-icon://home"></IconTabFilter>
                </items>
            </IconTabBar>
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer ></ToolbarSpacer>
                <Button text="Save" icon="sap-icon://save" type="Accept" press="save"></Button>
                <Button text="Cancel" icon="sap-icon://cancel" type="Reject" press="onCancel"></Button>
            </Toolbar>
        </footer>
    </Page>
</mvc:View>
