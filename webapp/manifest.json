{
  "sap.app": {
    "id": "myapp",
    "type": "application",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "CA-UI5-CON",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "i18n": "i18n/i18n.properties"
  },
  "sap.ui": {
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true

    },
    "technology": "UI5"
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.100.2",
      "libs": {
        "sap.m": {
          "minVersion": "1.100.2"
        },
        "sap.ui.core": {
          "minVersion": "1.100.2"
        },
      
        "sap.ui.layout": {
          "minVersion": "latest"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "id": "root",
      "viewName": "myapp.view.Main",
      "type": "XML"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewPath": "myapp.view",
        "viewType": "XML",
        "controlId": "app",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "name": "main",
          "pattern": "",
          "target": "main"
        }
      ],
      "targets": {
        "main": {
          "viewName": "View1",
          "viewId": "view1"
        }
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/mockdata/production.json"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleUrl": "i18n/i18n.properties"
        }
      }
    }
  }
}
